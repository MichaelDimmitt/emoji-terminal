set t_vb=
sha='$(if [ -d .git ]; then git rev-parse HEAD 2>/dev/null | cut -c -9; else echo "no git"; fi)'
branch='$(if [ -d .git ]; then git branch 2>/dev/null | grep "*" | cut -c 3-; else echo "detected"; fi)'
promptString(){
  emoji="\ndir: \w  \` ask_question $1 $2 \`
(\[\e[1;36m\]$sha\[\e[0m\]) (\[\e[1;36m\]$branch\[\e[0m\])\n"
}
# (\` blue $sha \`) (\` blue $branch \`)\n"

ask_question(){
  if [[ $? == 0 ]]; 
    then echo $1; 
    else echo $2; 
  fi 
}
